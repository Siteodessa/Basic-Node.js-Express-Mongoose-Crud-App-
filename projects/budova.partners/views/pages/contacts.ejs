



              <% function ifDefined(elem, markup, elser) {
                if   (typeof(elem) !== 'undefined' )  {
                  return markup
                } else {
                  return elser
                }
              }
              %>


<!DOCTYPE html>
<html lang="en">
<head>
<title><%= sitedata.contacts_page_title %></title>
