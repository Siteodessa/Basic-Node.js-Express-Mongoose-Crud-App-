	<div class="contact">
		<div class="container">
			<div class="row">
				<div class="col-lg-6 contact_col">
					<div class="estate_contact_form">
						<div class="contact_title"><%= sitedata.contacts_page_form_title %></div>
						<div class="estate_contact_form_container">
							<form id="estate_contact_form" class="estate_contact_form" action="post">
								<input id="estate_contact_form_name" class="estate_input_field estate_contact_form_name" type="text" placeholder="Имя" required="required" data-error="Name is required.">
								<input id="estate_contact_form_email" class="estate_input_field estate_contact_form_email" type="email" placeholder="E-mail" required="required" data-error="Valid email is required.">
								<input id="estate_contact_form_subject" class="estate_input_field estate_contact_form_subject" type="email" placeholder="Тема" required="required" data-error="Subject is required.">
								<textarea id="estate_contact_form_message" class="estate_text_field estate_contact_form_message" name="message" placeholder="Сообщение" required="required" data-error="Please, write us a message."></textarea>
								<button id="estate_contact_send_btn" type="submit" class="estate_contact_send_btn trans_200" value="Submit">Отправить</button>
							</form>
						</div>
					</div>
				</div>

				<div class="col-lg-3 contact_col">
					<div class="contact_title"><%= sitedata.contacts_page_contact_title %></div>
					<ul class="contact_info_list estate_contact">
						<%- ifDefined(sitedata.address, `
								<li class="contact_info_item d-flex flex-row">
								<div>
									<div class="contact_info_icon">
										<img src="images/placeholder.svg" alt="">
									</div>
								</div>
									<div class="contact_info_text">${sitedata.address}</div>
								</li>
						 `) %>


						<%- ifDefined(sitedata.phone, `
								<li class="contact_info_item d-flex flex-row">
								 <div>
									 <div class="contact_info_icon">
										<img src="images/phone-call.svg" alt="">
									 </div>
								 </div>
								 <div class="contact_info_text">${sitedata.phone}</div>
								</li>
						 `) %>

						<%- ifDefined(sitedata.mail, `
							<li class="contact_info_item d-flex flex-row">
								<div><div class="contact_info_icon"><img src="images/message.svg" alt=""></div></div>
								<div class="contact_info_text"><a href="mailto:${sitedata.mail}?Subject=Hello" target="_top">${sitedata.mail}</a></div>
							</li>
							`) %>
							<li class="contact_info_item d-flex flex-row">
								<div><div class="contact_info_icon"><img src="images/planet-earth.svg" alt=""></div></div>
								<div class="contact_info_text"><a href="/"><%- ifDefined(sitedata.sitename, sitedata.sitename) %></a></div>
							</li>
					</ul>
					<div class="estate_social">
						<ul class="estate_social_list">

							            <%- 		ifDefined(sitedata.fb_link, '<li class="footer_social_item"><a href="#"><i class="fab fa-facebook-f"></i></a></li>') %>
							            <%- 		ifDefined(sitedata.insta_link, '<li class="footer_social_item"><a href="#"><i class="fab fa-twitter"></i></a></li></a></li>') %>
							            <%- 		ifDefined(sitedata.twitter_link, '<li class="footer_social_item"><a href="#"><i class="fab fa-instagram"></i></a></li></a></li>') %>

						</ul>
					</div>
				</div>
				<div class="col-lg-3 contact_col">
					<div class="contact_title">about</div>
					<div class="estate_about_text">
