<!-- Content Header (Page header) -->

<style>
div#example1_filter {
    float:  right;
}span.add_new_obj {
    margin: 0 6px;
}td.sorting_1 {
    text-align: center;
}
img.obj {
  min-width: 60px;
      padding: 0;
      width: auto;
      height: auto;
      min-height: 60px;
      max-height: 60px;
      margin: 0 -3px;
      max-width: 60px;
}.roundy {
    border-radius:  100%;
    overflow:  hidden;
    border: 2px solid #eee;
}</style>
<section class="content-header">
  <h1>
    <%= model_label %>
    <small>просмотр</small>
  </h1>
  <ol class="breadcrumb">
    <li><a href="#"><i class="fa fa-dashboard"></i> Главная</a></li>
    <li><a href="#">    <%= model_label %></a></li>
    <li class="active">Просмотр</li>
  </ol>
</section>

<!-- Main content -->
<section class="content">
  <div class="row">
    <div class="col-xs-12">

      <!-- /.box -->

      <div class="box">
        <div class="box-header">
          <h3 class="box-title">Все     <%= model_label %></h3>
<span class="add_new_obj">
            <% include ./add_new_obj  %>
          </span>

        </div>
        <!-- /.box-header -->
        <div class="box-body">
          <table id="example1" class="table table-bordered table-striped">
            <thead>
            <tr>
              <th>image</th>
              <th>title</th>
              <th>company_name</th>
              <th>min_players</th>
              <th>max_players</th>
              <th>price</th>
              <th>complexity</th>
              <th>fear_level</th>
              <th>age</th>
              <th>description</th>


            </tr>
            </thead>
            <tbody>



 <% content.forEach(function(obj){ %>

  <tr id="<%= obj._id %>">



 <td> <img class="obj" src="<%= obj.home_background %>" alt="User profile picture"></td>
 <td> <%= obj.title %></td>

  <td> <%= obj.company_name %></td>
  <td> <%= obj.min_players %></td>
  <td> <%= obj.max_players %></td>
  <td> <%= obj.price %></td>
  <td> <%= obj.complexity %></td>
  <td> <%= obj.fear_level %></td>
  <td> <%= obj.age %></td>
  <td> <%= obj.description %></td>


               </tr>
 <% }); %>


            </tbody>
            <tfoot>
            <tr>
              <th>image</th>
              <th>title</th>
              <th>company_name</th>
              <th>min_players</th>
              <th>max_players</th>
              <th>price</th>
              <th>complexity</th>
              <th>fear_level</th>
              <th>age</th>
              <th>description</th>

            </tr>
            </tfoot>
          </table>
        </div>
        <!-- /.box-body -->
      </div>
      <!-- /.box -->
    </div>
    <!-- /.col -->
  </div>
  <!-- /.row -->
</section>
<!-- /.content -->
<script>


jQuery('img.obj').each(function(){
  jQuery(this).wrap('<div class="roundy"> </div>')
})
</script>
