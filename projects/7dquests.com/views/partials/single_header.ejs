
<% let toning; %>
<% if (d.header_type == 'Без') { toning = 'no-toning' }  else  {   toning = 'toning' } %>

<section class="banner-area relative home" id="home" style="background:url('/<%= d.single_image %>') ;    background-attachment: fixed; background-size:cover ;background-repeat: no-repeat">
  <div class="overlay <%= toning %> overlay-bg"></div>
  <div class="container">
    <div class="row d-flex align-items-center justify-content-center">
      <div class="about-content col-lg-12">
        <h1 class="text-white">

        <%= d.quest_name %>
        </h1>
        <p class="text-white link-nav">
          <a href="/">Главная </a>
           <span class="lnr lnr-arrow-right"></span>
          <a href="/quests">Квест-комнаты </a>
           <span class="lnr lnr-arrow-right"></span>
            <a href="/<%= d.page_link %>"> <%= d.quest_name %></a></p>
      </div>
    </div>
  </div>
</section>
